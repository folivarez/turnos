  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Castraciones Admin
        <small>Control panel</small>
      </h1>
     
    </section>

    <!-- Main content -->
    <section class="content">
      <!-- Small boxes (Stat box) -->
      <div class="row">
        <div class="col-lg-3 col-xs-6">
          <!-- small box -->
          <div class="small-box bg-aqua">
            <div class="inner">
              <h3><%= totalJornadas %></h3>
              <p>Jornadas</p>
            </div>
            <div class="icon">
              <i class="ion ion-calendar"></i>
            </div>
            
          </div>
        </div>
        <!-- ./col -->
        <div class="col-lg-3 col-xs-6">
          <!-- small box -->
          <div class="small-box bg-yellow">
            <div class="inner">
              <h3><%= totalTurnosSinConfirmar %></h3>

              <p>Turnos sin Confirmar</p>
            </div>
            <div class="icon">
              <i class="far fa-question-circle"></i>
            </div>
            
          </div>
        </div>
        <!-- ./col -->
        <div class="col-lg-3 col-xs-6">
          <!-- small box -->
          <div class="small-box bg-green">
            <div class="inner">
              <h3><%= totalTurnosConfirmados %></h3>

              <p>Turnos Confirmados</p>
            </div>
            <div class="icon">
              <i class="far fa-check-square"></i>
            </div>
            
          </div>
        </div>
        <!-- ./col -->
        <div class="col-lg-3 col-xs-6">
          <!-- small box -->
          <div class="small-box bg-red">
            <div class="inner">
              <h3>
              	
              		<%= totalTurnosCancelados %>
              	
			</h3>
              <p>Turnos Cancelados</p>
            </div>
            <div class="icon">
              <i class="fas fa-ban"></i>
            </div>
            
          </div>
        </div>
        <!-- ./col -->
      </div>
      <!-- /.row -->

       <!-- /.row -->
      <div class="row">
        <div class="col-xs-12">
          <div class="box">
            <div class="box-header">
              <h3 class="box-title">Jornadas Activas</h3>

             
            </div>
            <!-- /.box-header -->
            <div class="box-body table-responsive no-padding">
              <table class="table table-hover">
                
                        <thead>
                          <tr>
                            <th scope="col">Localidad</th>
                            <th scope="col">Fecha</th>
                            <th scope="col">Direccion</th>
                            <th scope="col">Direccion Parcial</th>
                            <th scope="col">Precio</th>
                            <th scope="col">Turnos 1/2 hs</th>
                            <th scope="col">Turno</th>
                            
                          </tr>
                        </thead>
                        <tbody>
                          
                        <% jobs.forEach(function(data) { %>                              
                               <tr>
                                  <td><a href="/castraciones/jornadas/<%= data._id  %>  "><%= data.localidad %></a></td>
                                  <td> <%= moment(data.fecha).format('D/M/YYYY HH:mm'); %> Hs. </td> 
                                  <td><%= data.direccion %></td> 
                                  <td><%= data.direparcial %></td> 
                                  <td>$<%= data.precio %></td> 
                                  <td><%= data.cant_grupo %></td>
                                  <td><%= moment(data.hora_prox_turno).format('HH:mm'); %> Hs. </td> 
                                  <td><a data="<%= data._id  %>"href=""><i title="Editar" class="far fa-edit"></i></a>  </td>
                                  <td><a href=""><i title="Suspender" class="fas fa-ban"></i></a>  </td>
                                  <td><a href=""><i title="48 horas" class="far fa-envelope"></i></a>  </td> 

                                  <td><a href="#" class="eliminar-jornada" data-id="<%= data._id  %>"  data-localidad="<%= data.localidad %>" data-toggle="modal" data-target="#modal-danger"> <i title="Eliminar" class="far fa-trash-alt"></i></a></td>
                              </tr>
                        <% }); %>
                        </tbody>
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
      </div>

     

     
          
        </section>

    



     <div class="modal modal-danger fade modal-eliminar " id="modal-danger">
          <div class="modal-dialog">
            <div class="modal-content">
              <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">ATENCION</h4>
              </div>
              <div class="modal-body" id="modal-body-eliminar">
                
              </div>
              <div class="modal-footer">
                <button type="button" class="btn btn-outline pull-left" data-dismiss="modal">Salir</button>
                <button type="button" class="btn btn-outline confirma-eliminar">ELIMINAR</button>
              </div>
            </div>
            <!-- /.modal-content -->
          </div>
          <!-- /.modal-dialog -->
        </div>
        <!-- /.modal -->





        

       
      
  </div>
  <!-- /.content-wrapper -->
