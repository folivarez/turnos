<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">

   <!-- Header  -->
    
      <% include ../templates/header %>


      <!-- Aside  -->

      <% include ../templates/aside %>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
       Veterinarios
      </h1>
     
    </section>

    <!-- Main content -->
    <section class="content">
     
      <!-- /.row -->

       <!-- /.row -->
      <div class="row">
        <div class="col-md-6">
          <div class="box">
            <div class="box-header">
              <h3 class="box-title"> Dar de Alta</h3> 
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              <form name="form1" >

                        <div class="form-group">
                            <label for="nombre">Nombre</label>
                            <input type="text" class="form-control form-control-sm" name="nombre" id="nombre" >
                            
                        </div>  
                        <div class="form-group">
                            <label for="adicional">Adicional </label>
                            <input type="number" class="form-control form-control-sm" name="adicional" id="adicional" >
                        </div>  
                        <div class="form-group">
                            <label for="puntos">Puntos </label>
                            <select class="form-control form-control-sm" for="puntos" name="puntos" id="puntos">
                                  <option value="1">Intradermicos</option>
                                  <option value="0">Externos</option>
                            </select>
                         </div>   
                            <button type="submit" id="guardar-veterinario"class="btn btn-primary">Guardar Veterinario</button>

              </form>         
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>

         <div class="col-md-12">
          <div class="box">
            <div class="box-header">
              <h3 class="box-title"> Lista de Veterinarios</h3> 
            </div>
           
               <div class="box-body table-responsive no-padding">
                      <table id="listaTurno" class="listaTurnoMobile table table-hover ">
                          <thead>
                              <tr>
                                  <th scope="col">Nombre</th>
                                  <th scope="col">Adicional</th>
                                  <th scope="col">Puntos</th>

                              </tr>
                          </thead>
                          <tbody>
                              <% lista.forEach(function(veterinarios) { %>
                              <tr>
                                <td><%= veterinarios.nombre %></td>
                                <td>$<%= veterinarios.adicional %></td>
                                 <% if (veterinarios.puntos == 0) { -%>
                                      <td>Externos</td>
                                 <% } else { -%>
                                      <td>Intradermicos</td>
                                 <% } -%>
                                 <td> <a href="#" class="eliminar-veterinario" data-id="<%= veterinarios._id  %>"> Eliminar</a></td>
                              </tr>
                              <% }); %>
                          </tbody>
                      </table>
                </div>
          </div>
          <!-- /.box -->
        </div>

      </div>
     
          
        </section>
        <!-- /.Left col -->
        <!-- right col (We are only adding the ID to make the widgets sortable)-->
     
        <!-- right col -->
      </div>
      <!-- /.row (main row) -->

    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
