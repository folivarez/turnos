<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">

  <!-- Header  -->
    
      <% include ../templates/header %>


      <!-- Aside  -->

      <% include ../templates/aside %>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
       Alta
        <small>de jornada</small>
      </h1>
     
    </section>

    <!-- Main content -->
    <section class="content">
     
      <!-- /.row -->

       <!-- /.row -->
      <div class="row">
        <div class="col-md-6">
          <div class="box">
            <div class="box-header">
              <h3 class="box-title"> Crear Jornada</h3>

             
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              
              <form name="form1" >

                        <div class="form-group">
                            <label for="localidad">Localidad </label>
                            <input type="text" class="form-control form-control-sm" data-id="<%= jornada.id%>" name="localidad" id="localidad" value="<%= jornada.localidad%>" >
                            <small id="emailHelp" class="form-text text-muted">Ingrese la localidad de la Jornada</small>
                        </div>  
                         <div class="form-group">
                            <label for="fecha">Fecha </label>
                            <input type="datetime-local" class="form-control form-control-sm" value="<%= moment(jornada.fecha).format('YYYY-MM-DDTHH:mm');%>" name="fecha" id="fecha-jornada" disabled>
                            <!-- 2019-01-16T08:00 -->
                        </div>  
                         <div class="form-group">
                            <label for="direccion">Dirección </label>
                            <input type="text" class="form-control form-control-sm" name="direccion" id="direccion" value="<%= jornada.direccion%>" >
                        </div>  

                        <div class="form-group">
                            <label for="direparcial">Dirección Parcial</label>
                            <input type="text" class="form-control form-control-sm" name="direparcial" id="direparcial" value="<%=jornada.direparcial %>" >
                        </div>  

                         <div class="form-group">
                            <label for="precio">Precio </label>
                            <input type="number" class="form-control form-control-sm" name="precio" id="precio" value="<%= jornada.precio%>" >
                        </div>  
                         <div class="form-group">
                            <label for="cant_grupo">Turnos cada 1/2 hora</label>
                            <input type="number" class="form-control form-control-sm" name="cant_grupo" id="cant_grupo" value="<%= jornada.cant_grupo%>" >
                        </div>   

                         <div class="form-group">
                            <label for="activa">Veterinario </label>
                            <select class="form-control form-control-sm" for="activa" name="activa" id="veterinario">
                              <option value="<%= jornada.veterinario %>"><%= jornada.veterinario %></option>
                              <option value="SinVeterinario">Sin Veterinario</option>

                               <% data.forEach(function(veterinarios) { %>
                             
                                <%= veterinarios.nombre %>
                                <option value="<%= veterinarios.nombre %>"><%= veterinarios.nombre %></option>
                               
                              <% }); %>
                                  
                            </select>
                         </div>   

                         <div class="form-group">
                            <label for="observacion">Observacion</label>
                            <input type="text" class="form-control form-control-sm" name="observacion" id="observacion" value="<%= jornada.observacion%>" >
                            <small id="emailHelp" class="form-text text-muted"> (sera visible al solicitar turno y confirmar turno).</small>
                        </div>  

                            <button type="submit" id="guardar-jornada"class="btn btn-primary">Guardar</button>

                    </form>         
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>

      </div>
     
          
        </section>
        <!-- /.Left col -->
        <!-- right col (We are only adding the ID to make the widgets sortable)-->
     
        <!-- right col -->
      </div>
      <!-- /.row (main row) -->

    </section>
    <!-- /.content -->
