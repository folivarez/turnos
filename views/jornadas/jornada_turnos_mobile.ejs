<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
         <!-- Header  -->
    
      <% include ../templates/header %>


      <!-- Aside  -->

      <% include ../templates/aside %>

        
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
       Turnos
        <small>por jornada</small>
      </h1>
            </section>
            <!-- Main content -->
            <section class="content">
                <!-- /.row -->
                <!-- Small boxes (Stat box) -->
                <div class="row">
                    <!--    <div class="col-lg-3 col-xs-6">
                  <div class="small-box bg-aqua">
            <div class="inner">
              <h3></h3>
              <p>Jornadas</p>
            </div>
            <div class="icon">
              <i class="ion ion-calendar"></i>
            </div>
            
          </div>
        </div> -->
                    <!-- ./col -->
                    <div class="col-lg-2 col-xs-4">
                        <!-- small box -->
                        <div class="small-box bg-yellow">
                            <div class="inner">
                                <h3><%= totalTurnosSinConfirmar %></h3>
                                <p>Turnos sin Confirmar</p>
                            </div>
                            <div class="icon">
                                <i class="far fa-question-circle"></i>
                            </div>
                        </div>
                    </div>
                    <!-- ./col -->
                    <div class="col-lg-2 col-xs-4">
                        <!-- small box -->
                        <div class="small-box bg-green">
                            <div class="inner">
                                <h3><%= totalTurnosConfirmados %></h3>
                                <p>Turnos Confirmados</p>
                            </div>
                            <div class="icon">
                                <i class="far fa-check-square"></i>
                            </div>
                        </div>
                    </div>
                    <!-- ./col -->
                    <div class="col-lg-2 col-xs-4">
                        <!-- small box -->
                        <div class="small-box bg-red">
                            <div class="inner">
                                <h3>

                  <%= totalTurnosCancelados %>
                
               </h3>
                                <p>Turnos Cancelados</p>
                            </div>
                            <div class="icon">
                                <i class="fas fa-ban"></i>
                            </div>
                        </div>
                    </div>
                    <!-- ./col -->
                    <div class="col-lg-6 col-xs-12">
                        <div class="small-box bg-black">
                            <div class="inner">
                            </div>
                            <div class="box-body no-padding">
                                <table class="table ">
                                    <p class="text-center">Animales Confirmados</p>
                                    <thead>
                                        <tr>
                                            <th class="text-center" scope="col"><i class="fas fa-cat  text-blue"></i></th>
                                            <th class="text-center" scope="col"><i class="fas fa-cat  text-fuchsia"></i></th>
                                            <th class="text-center" scope="col"><i class="fas fa-dog  text-blue"></i></th>
                                            <th class="text-center" scope="col"><i class="fas fa-dog  text-fuchsia"></i></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <td class="text-center">
                                            <%=totalGatos%>
                                        </td>
                                        <td class="text-center">
                                            <%=totalGatas%>
                                        </td>
                                        <td class="text-center">
                                            <%=totalPerros%>
                                        </td>
                                        <td class="text-center">
                                            <%=totalPerras%>
                                        </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /.row -->
                <!-- /.row -->
                <div class="row">
                    <div class="col-xs-12">
                        <div class="box">
                            <div class="box-header">
                                <h3 class="box-title"> Jornada: <b><%= jornada.localidad %></b>  <%= moment(jornada.fecha).format('D/M/YYYY HH:mm');%> | Veterinario: <%= jornada.veterinario %> </h3>
                               
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body table-responsive no-padding">
                                <table id="listaTurno" class="listaTurnoMobile table table-hover ">
                                    <thead>
                                        <tr>
                                            <th scope="col"></th>
                                            <th scope="col">Nombre</th>
                                            <th scope="col">Asistio</th>
                                            <th scope="col">Turno</th>
                                            <th scope="col">DNI</th>
                                            <th scope="col">orden turno</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% turnosLista.forEach(function(turnos) { %>
                                        
                                        <% if (turnos.animal.peso == "Mas de 30 Kg") { %>

                                        <tr class="table-warning">
                                        
                                        <% } else {  %>
 
                                        <tr>

                                        <% } %>

                                            <% if (turnos.confirmado == 1) { %>
                                                <td class="bg-green">Si</td>
                                                <% } 
                                                else { 
                                                    if (turnos.confirmado == "") { %>
                                                        <td class="bg-yellow">Pe</td>
                                                    <% } else {  
                                                               if (turnos.confirmado == 0) {%>
                                                                    <td class="bg-red">NO</td>
                                                                <% }
                                                            }
                                                    } %>
                                    
                                            <td>
                                                <%= turnos.nombre %>
                                            </td>

                                            <% if (turnos.asistio == '') { %>
                                              <td class="rojo"><a  data-estado="ausente" data-id="<%= turnos.id %>" class="estadoPersonaMobile postoperatorio presente" data-veterinarionombre="<%= jornada.veterinario %>" data-localidad="<%= jornada.localidad %>" data-observacion="<%= jornada.observacion %>" data-hora="<%= moment(turnos.hora).format('HH:mm'); %>" data-telefono="<%= turnos.telefono %>" data-nombre="<%= turnos.nombre %>" data-mascota="<%= turnos.animal.nombreMascota %>" data-fecha="<%= moment(turnos.hora).format('D/M/YYYY'); %>"  ><i class="fas fa-toggle-off fa-2x"></i></a></td>
                                            <% } else { %>
                                              <td class="verde"><a data-estado="presente"  data-veterinarionombre="<%= jornada.veterinario %>"data-localidad="<%= jornada.localidad %>" data-mascota="<%= turnos.animal.nombreMascota %>" data-observacion="<%= jornada.observacion %>" data-hora="<%= moment(turnos.hora).format('HH:mm'); %>" data-telefono="<%= turnos.telefono %>" data-nombre="<%= turnos.nombre %>" data-fecha="<%= moment(turnos.hora).format('D/M/YYYY'); %>" data-id="<%= turnos.id %>" class="estadoPersonaMobile postoperatorio"><i class="fas fa-toggle-on fa-2x"></i></a></td>
                                            <% } %>
                                            <td>
                                                <%= moment(turnos.hora).format('HH:mm'); %> hs.</td>
                                                 <td>
                                                    <%= turnos.dni %>
                                                </td>
                                                <td>
                                                <%= moment(turnos.reserva).format('D/M HH:mm '); %> hs.</td>
                                        </tr>
                                        <% }); %>
                                    </tbody>
                                </table>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /.box -->
                    </div>
                </div>
            </section>
            <!-- /.Left col -->
            <!-- right col (We are only adding the ID to make the widgets sortable)-->
            <!-- right col -->
        </div>
        <!-- /.row (main row) -->
        </section>
        <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->
