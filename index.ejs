                         -->
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="myModalCompleto" class="modal fade meet-us" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <a href="#" class="close" data-dismiss="modal" aria-hidden="true">
            <span class="fa fa-close close-1"></span>
            <span class="fa fa-close close-2"></span>
        </a>
        <div class="modal-body">
            <div class="container">
                <div class="row modal-body-content">
                    <div class=" col-xs-12 ">
                        <div class="col-md-12 col-xs-12 text-center ">
                            <i class="far fa-frown fa-9x"></i>
                            <h2>La Jornada se encuentra COMPLETA!</h2>
                            <h3>Por favor verifique otra fecha.</h3>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Javascript -->
    <script type="text/javascript" src="js/jquery-1.11.0.min.js"></script>
    <script type="text/javascript" src="bootstrap/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="js/detectmobilebrowser.js"></script>
    <script type="text/javascript" src="js/supersized.3.2.7.min.js"></script>
    <script type="text/javascript" src="js/supersized.shutter.min.js"></script>
    <script type="text/javascript" src="js/jquery.easing.1.3.js"></script>
    <script type="text/javascript" src="js/jquery.scrollTo.min.js"></script>
    <script type="text/javascript" src="js/jquery.countdown.js"></script>
    <script type="text/javascript" src="js/waypoints.min.js"></script>
